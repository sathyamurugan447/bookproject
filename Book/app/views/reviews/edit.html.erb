<div class="col-md-6 col-md-offset-3">
  <div class="edit-note-form">


    <h1>Edit Review</h1>

    <%= simple_form_for([@note, @review]) do |f| %>
      <div id="rating-form">
   	    <%= f.input :rating %>
   	
   	  </div>
   	  
      <%= f.input :comment %>
      <%= f.button :submit, :class => "btn-custom2" %>
    <% end %>
  </div>
</div>
<script>
  $('.review-rating').raty({
	path: '/assets/'
	scoreName: 'review[rating]'
	
  });
</script>