<div class="col-md-6 col-md-offset-3">
  <div class="edit-note-form">


    <h1>Edit Book</h1>
    <%= simple_form_for @note, :html => { :multipart => true} do |f| %>
      <%= f.select :category_id, @categories %>
      <%= f.file_field :note_img %>
      <%= f.input :title, lable: "Note Title" %>
      <%= f.input :description %>
      <%=f.input :author %>
      <%= f.button :submit %>
    <% end %>
  </div>

</div>

